	<queryString>
		<![CDATA[--2.28	R-HBM-031 Letter - 特別配額申請公屋及遷離部門宿舍(Retired)(有RA) + 同意書(Chinese)
with contact(name, name_c, post_mobile, post_talno) as (
--15. st.Name
select ST.NAME, st.chinese name_c, ep.POST_MOBILE, ep.post_telno
from posting_now pn inner join staff st on pn.STAFF_NO = st.STAFF_NO and (st.UPDATE_MODE is null or st.UPDATE_MODE <> 'D')
left outer join esm_post ep on pn.POST_ID = ep.post_id and ep.status = 'A'
where 
-- ISNULL(duties_X,ISNULL(duties_T,'')) = 'CO(Q&S)' and 
pn.staff_no =  $P{contactPerson} ),
inst_contact(name, name_c, contact_person, contact_person_c) as (
--16.st.name, 17.Head of Institution of the quartering office
select ST.NAME, st.chinese name_c, hoi.CONTACT_PERSON, hoi.C_INSTITUTION_CONTACT_PERSON contact_person_c
from posting_now pn 
inner join staff st on pn.STAFF_NO = st.STAFF_NO 
	and (st.UPDATE_MODE is null or st.UPDATE_MODE <> 'D')
inner join HEAD_OF_INSTITUTION hoi on 
	ISNULL(EXACT_INST,ISNULL(INST_R,ISNULL(INST_X,ISNULL(INST_T,'')))) = hoi.INSTITUTION COLLATE SQL_Latin1_General_CP1_CI_AS
where 
-- ISNULL(duties_X,ISNULL(duties_T,'')) = 'CO(Q&S)' and 
pn.staff_no =  $P{staffNo} ),

signer(name_c) as (
select st.chinese name_c  from staff st 
inner join posting_now pn on st.STAFF_NO = pn.STAFF_NO
where 
--ISNULL(duties_X,ISNULL(duties_T,'')) = 'EO(P)1' and 
st.staff_no =  $P{signer} )

select 
a.staff_no, a.ctitle, a.chinese, a.surname_c, a.cPost,
(select chinese from ranks r where r.rankabbr = a.rankabbr) rank, 
b.ha_letter_date, 
b.sign_tncy_agm_date, b.sign_tncy_agm_date2, 
b.ptqaddress, b.phaddress,
(select name_c from contact) as q_officer,
(select name_c from signer) as signer, 
(select name_c from inst_contact) as cc_1,
(select contact_person_c from inst_contact) as cc_2,
b.year, b.code
from (
--1.cPost, 2,21.rankabbr, 3.staffNo, 4,20. chinese, 5.ctitle
SELECT st.staff_no, st.rankabbr, sn.chinese_surname surname_c,
case when st.sex = 'M' then N'先生' else N'女士' end ctitle,
concat(ISNULL(EXACT_INST,ISNULL(INST_R,ISNULL(INST_X,ISNULL(INST_T,'')))) , '/',
ISNULL(section_R,ISNULL(section_X,ISNULL(section_T,''))), '/', ISNULL(duties_X,ISNULL(duties_T,'')))
AS cPost, st.chinese FROM staff st 
left outer join POSTING_NOW pn on st.STAFF_NO = pn.STAFF_NO
left outer join staff_name sn on st.staff_no = sn.staff_no
where st.STAFF_NO =  $P{staffNo} ) a 
left join (
--8. ha_letter_date, 9,12,19.address, 10.SIGN_TNCY_AGM_DATE, 11,14.SIGN_TNCY_AGM_DATE2
--13 is unknown
select top 1 ha.staff_no, concat(flat, floor, room, block, house, building, qa.ESTATE, tower, street, qa.DISTRICT, region) as ptqaddress, 
ha.ha_letter_date,
st.CHINESE, case when st.sex = 'M' then N'先生' else N'女士' end ctitle, 
ha.year, hb.quarters_code code,  hb.ADDRESS as phAddress, hhc.SIGN_TNCY_AGM_DATE, 
dateadd(month, 2, hhc.SIGN_TNCY_AGM_DATE) as SIGN_TNCY_AGM_DATE2
from HOUSING_APP ha 
inner join staff st on ha.STAFF_NO = st.STAFF_NO 
inner join housing_benefit hb on ha.STAFF_NO = hb.STAFF_NO 
--and hb.BENEFIT = 'PH'
inner join hbm_hb_csph hhc on hhc.BENEFIT_ID = hb.RECORD_ID
inner join personal p on st.STAFF_NO = p.STAFF_NO
inner join staff_Name sn on st.STAFF_NO = sn.staff_no
inner join csdqas..quarters_user qu on ha.STAFF_NO = qu.STAFF_NO 
--and qu.DATE_OUT is null and (qu.DELFLAG is null or qu.DELFLAG <> 'D')
inner join csdqas..quarters q on qu.code = q.code 
--and q.type in ('J', 'O') and (q.DELFLAG is null or q.DELFLAG <> 'D')
inner join QASMGR..QUARTERS_ADDRESS qa on q.code = qa.code
where upper(quotas) = 'SPECIAL' and CANCEL_CODE = 20 and (st.UPDATE_MODE is null or st.UPDATE_MODE <> 'D')
--and exists (select * from HOUSING_BENEFIT hb where BENEFIT = 'RA' and hb.STAFF_NO = ha.staff_no)
and ha.staff_no =  $P{staffNo} ) b on  a.staff_no = b.staff_no]]>
